<h1>Confirm Page</h1>

<ul>
	<% characters.forEach(function(character) { %>
		<li>
			<form action="/character/confirm" method="POST">
				<input type="hidden" name="name" value="<%= character.name %>">
				<input type="hidden" name="_id" value="<%= character._id %>">
				<input type="hidden" name="gender" value="<%= character.gender %>">
				<input type="submit" class="btn btn-dark" value="<%= character.name %>">
			</form>
		</li>
		<br>
	<% }) %>
</ul>